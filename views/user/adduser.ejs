<main class="col-sm-9 offset-sm-3 col-md-10 offset-md-2 pt-3">
	<h1><%= title %></h1>

	<section class="row placeholders">

	<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
		<form id='adduser-form'>
			<div class="form-group">
				<label for="">请输入要添加的学号，若输入多个请按回车(Enter)隔开</label>
				<textarea name="users" class="form-control" type="text" rows="6"></textarea>
			</div>
			<div class="form-group">
				<label for="">请选择部门</label>
				<select id="department" name="department" class="form-control">
					<% departments.forEach(function (element) { %>
						<option value="<%= element %>"><%= element %></option>
					<% }) %>
				</select>
		</div>
			<div class="form-group">
				<label for="">请选择职位级别</label>
				<select id="department" name="position" class="form-control">
					<% positions.forEach(function (element) { %>
						<option value="<%= element %>"><%= element %></option>
					<% }) %>
				</select>
		</div>
			<button type="button" id="adduser-button" class="btn btn-primary">保存</button>
		</form>
	</div>
	</section>

</main>
<script src="/dist/js/addusers.js"></script>
